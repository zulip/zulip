{{#with muted_user}}
<tr data-user-id="{{user_id}}" data-user-name="{{user_name}}" data-date-muted="{{date_muted_str}}">
    <td>{{user_name}}</td>
    <td>{{date_muted_str}}</td>
    <td class="actions">
        {{#if can_unmute}}
        {{> ./components/action_button
          label=(t "Unmute")
          attention="quiet"
          intent="danger"
          custom_classes="settings-unmute-user"
          }}
        {{/if}}
    </td>
</tr>
{{/with}}
