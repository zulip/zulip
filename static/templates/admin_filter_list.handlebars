{{#with filter}}
<tr class="filter_row">
  <td>
      <span class="filter_pattern">{{pattern}}</span>
  </td>
  <td>
      <span class="filter_url_format_string">{{url_format_string}}</span>
  </td>
  <td>
      <button class="btn delete btn-danger" data-filter-id="{{id}}">
        {{t "Delete" }}
      </button>
  </td>
</tr>
{{/with}}
