{{#with stream}}
<tr class="stream_row">
    <td>
        {{#if invite_only}}<i class="icon-vector-lock "></i>{{/if}}
        <span class="stream_name">{{name}}</span>
    </td>
    <td>
        <button class="button deactivate btn-danger">
            {{t "Delete stream" }}
        </button>
    </td>
</tr>
{{/with}}
